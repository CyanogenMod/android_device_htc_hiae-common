# Audio
audio.offload.buffer.size.kb=1024
audio.offload.multiple.enabled=false
audio.offload.pcm.16bit.enable=true
audio.offload.pcm.24bit.enable=true
av.offload.enable=true

ro.qc.sdk.audio.fluencetype=fluence
ro.qc.sdk.audio.ssr=false
use.voice.path.for.pcm.voip=true

persist.audio.fluence.voicecall=true
persist.audio.fluence.voicerec=false
persist.audio.fluence.speaker=true

audio.ds1.metainfo.key=273
dmid=-1286820014

# Bluetooth
ro.bt.bdaddr_path=/sys/module/htc_bdaddress/parameters/bdaddress

# Camera
camera2.portability.force_api=1

# Display
persist.hwc.mdpcomp.enable=true
ro.opengles.version=196609
ro.sf.lcd_density=480

# Factory Reset Protection
ro.frp.pst=/dev/block/bootdevice/by-name/frp

# IO Scheduler
sys.io.scheduler=bfq

# NITZ
persist.rild.nitz_plmn=
persist.rild.nitz_long_ons_0=
persist.rild.nitz_long_ons_1=
persist.rild.nitz_long_ons_2=
persist.rild.nitz_long_ons_3=
persist.rild.nitz_short_ons_0=
persist.rild.nitz_short_ons_1=
persist.rild.nitz_short_ons_2=
persist.rild.nitz_short_ons_3=

# Perf
ro.vendor.extension_library=libqti-perfd-client.so

# Radio
rild.libargs=-d /dev/smd0
rild.libpath=/system/lib64/libhtc_rilhook.so
ro.baseband.arch=msm
ro.telephony.call_ring.multiple=false
ro.telephony.default_network=9
telephony.lteOnGsmDevice=1

persist.radio.process_sups_ind=1

ro.ril.disable.cpc=1

ro.ril.enable.a53=1
ro.ril.enable.sdr=1

ro.ril.hsdpa.category=14
ro.ril.hsupa.category=6
ro.ril.hsxpa=4

ro.ril.ltefgi=2131754170
ro.ril.ltefgi.rel10=1747189760
ro.ril.ltefgi.rel9=2147483648

ro.ril.oem.ecclist=911
ro.ril.oem.nosim.ecclist=911

# Wifi
wifi.interface=wlan0
